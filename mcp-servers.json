{
  "servers": {
    "google-calendar": {
      "source": "https://github.com/nspady/google-calendar-mcp",
      "install_path": "/opt/mcp-servers/google-calendar-mcp",
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@cocal/google-calendar-mcp"],
      "env_vars": ["GOOGLE_OAUTH_CREDENTIALS", "ENABLED_TOOLS"],
      "description": "Google Calendar integration via MCP",
      "managed_by": "mcporter",
      "mcporter_scope": "home"
    },
    "ticktick": {
      "source": "https://github.com/jacepark12/ticktick-mcp",
      "install_path": "/opt/mcp-servers/ticktick-mcp",
      "transport": "stdio",
      "command": "/opt/mcp-servers/ticktick-mcp/.venv/bin/python",
      "args": ["-m", "ticktick_mcp.cli", "run"],
      "env_vars": ["TICKTICK_ACCESS_TOKEN", "TICKTICK_CLIENT_ID", "TICKTICK_CLIENT_SECRET", "TICKTICK_TIMEZONE"],
      "description": "TickTick task management via MCP",
      "managed_by": "mcporter",
      "mcporter_scope": "home"
    },
    "zenmoney": {
      "source": "https://github.com/germanKoch/zenmoney-mcp",
      "install_path": "/opt/mcp-servers/zenmoney-mcp",
      "transport": "stdio",
      "command": "/opt/mcp-servers/zenmoney-mcp/.venv/bin/python",
      "args": ["/opt/mcp-servers/zenmoney-mcp/main.py"],
      "env_vars": [],
      "token_file": ".token.json",
      "description": "ZenMoney finance tracking via MCP",
      "managed_by": "mcporter",
      "mcporter_scope": "home"
    }
  }
}
